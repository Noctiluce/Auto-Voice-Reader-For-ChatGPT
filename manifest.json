{
  "manifest_version": 3,
  "name": "ChatGPT Auto Voice Reader",
  "version": "1.0",
  "description": "Automatically click the read outload button on ChatGPT.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Auto read ChatGPT"
  },
  "icons": {
    "48": "icons/icon48.png"
  }
}
